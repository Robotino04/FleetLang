let putchar_impl = (char: i32) -> i32 @extern "putchar";

let putchar = (char: u8) -> () {
    putchar_impl(char as i32);
}

let main = () -> i32 {
    putchar('H');
    putchar('e');
    putchar('l');
    putchar('l');
    putchar('o');
    putchar(',');
    putchar(' ');
    putchar('W');
    putchar('o');
    putchar('r');
    putchar('l');
    putchar('d');
    putchar('!');
    putchar('\n');
    return 0;
}
